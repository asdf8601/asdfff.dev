{
  "name": "asdfff.dev",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "node generate-search-data.js && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky",
    "clean": "rm -rf node_modules/.vite dist .astro",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "spell": "cspell '**/*.{js,ts,astro,json,md,mdx,css}'",
    "spell:fix": "cspell '**/*.{js,ts,astro,json,md,mdx,css}' --words-only --unique",
    "commitlint": "commitlint --edit",
    "validate": "bun run lint && bun run spell && bun run build"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.8",
    "@codemirror/commands": "^6.10.0",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/language": "^6.11.3",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.6",
    "@replit/codemirror-vim": "^6.3.0",
    "astro": "^5.14.5",
    "codemirror": "^6.0.2",
    "playwright": "^1.56.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "remark-toc": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "cspell": "^9.2.2",
    "eslint": "^9.39.0",
    "eslint-plugin-astro": "^1.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1"
  }
}
